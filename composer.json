{
    "name": "lithemod/cors",
    "description": "Configures Cross-Origin Resource Sharing (CORS) headers for your Lithe application.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "lithemod/flow": "^1.0"
    },
    "autoload": {
        "files": [
            "src/cors.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "license": "MIT"
}
